{
  "name": "graphql",
  "version": "1.0.0",
  "description": "A Node.js project",
  "author": "Toan Tran <toantdfu@gmail.com>",
  "main": "index.js",
  "engines": {
    "node": ">=8.5.0",
    "npm": ">=3.10.10",
    "yarn": ">=0.27.5"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node index.js",
    "dev": "nodemon index.js --exec babel-node",
    "dev:debug": "cross-env DEBUG=graphql:* yarn dev",
    "build": "",
    "lint": "esw *.js server config --color",
    "lint:fix": "esw *.js {server,docs} config --color --fix",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests/**/*.test.js --recursive --timeout 15000",
    "test:watch": "yarn test -- --watch"
  },
  "keywords": [
    "graphql",
    "node",
    "node.js",
    "postgreSQL",
    "es6",
    "mocha",
    "REST",
    "API"
  ],
  "dependencies": {
    "aws-sdk": "^2.103.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "1.7.0",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.3",
    "debug": "^3.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.5",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "helmet": "^3.6.1",
    "http-status": "^1.0.1",
    "joi": "^11.0.3",
    "jsonwebtoken": "^8.0.1",
    "method-override": "^2.3.5",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.3.0",
    "sequelize": "^4.2.1",
    "sequelize-cli": "^3.0.0",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.0.2",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "del": "^3.0.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-watch": "^3.1.2",
    "faker": "^4.1.0",
    "husky": "^0.14.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "run-sequence": "^2.0.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
